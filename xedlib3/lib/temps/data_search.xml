<null>

<results>
<result>
<table id="result:{{id}}">
	<tr><td colspan="2">
		<result_button><a class="button">{{text}}</a></result_button>
	</td></tr>
<result_field>
	<tr><td>{{text}}</td><td>{{val}}</td></tr>
</result_field>
</table>
</result>
</results>

<search>
<form action="{{root}}{{me}}/{{name}}/search" method="post">
<script type="text/javascript" src="{{root}}/xedlib/temps/data_search.js" />
<fieldset>
<legend>Select fields to search</legend>
<ul>
<searchfield>
	<li><input type="checkbox" id="hider_{{id}}" name="{{name}}_search[{{fname}}]" class="hider" />
	<label for="{{id}}">{{text}}</label>
	<div class="hidden ds_field" id="hidden_{{id}}">{{field}}</div></li>
</searchfield>
</ul>
</fieldset>
<input type="submit" value="Search" />
</form>
</search>

</null>