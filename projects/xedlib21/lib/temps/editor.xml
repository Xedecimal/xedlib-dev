<null>

<part type="ContentSearch">
	<box title="Locate and Edit {{plural}}">
	<form action="{{me}}">
	<input type="hidden" name="editor" value="{{name}}" />
	Search: <input type="text" name="{{name}}_q"
		onkeyup="$('#ssub').val(this.value.length > 0 ? 'Search' : 'Retrieve All');" />
	<input type="submit" value="Retrieve All" id="ssub" />
	</form>
	</box>
</part>

<if check="isset($this->vars[&quot;table&quot;])">
<box title="{{table_title}}">
{{table}}
</box>
</if>

<forms>
<box title="{{form_title}}">
{{form_content}}
</box>
</forms>

</null>